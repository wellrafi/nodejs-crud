<script>
  $("#dataTable").on('click', '.btn-update-jenis-peleko', function () {
    let id = $(this).data('id');
    let name = $(this).data('name');
    $('#edit-modal-jenis-peleko-name').val(name);
    $('#edit-modal-jenis-peleko-form').attr("action", "/admin/jenis-peleko/" + id + "?_method=PUT");
  })
  $("#dataTable").on('click', '.btn-delete-jenis-peleko', function () {
    let id = $(this).data('id');
    $('#delete-modal-jenis-peleko-form').attr("action", "/admin/jenis-peleko/" + id + "?_method=DELETE");
  })
  
</script>